<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
	<interface name="org.shadoware.xinx">
		<signal name="aboutToClose"/>

		<method name="newFile">
			<arg name="filename" type="s" direction="in"/>
		</method>
		<method name="newTemplate">
			<arg name="name" type="s" direction="in"/>
			<arg name="category" type="s" direction="in"/>
			<arg name="description" type="s" direction="in"/>
			<arg name="text" type="s" direction="in"/>
			<arg name="arguments" type="as" direction="in"/>
			<arg name="key" type="s" direction="in"/>
			<arg name="type" type="s" direction="in"/>
		</method>
		<method name="openFile">
			<arg name="filename" type="s" direction="in"/>
		</method>
		<method name="saveFileAs">
			<arg name="filename" type="s" direction="in"/>
		</method>
		<method name="saveAllFile"/>
		<method name="closeFile"/>
		<method name="closeAllFile"/>

		<method name="newProject">
			<arg name="name" type="s" direction="in"/>
			<arg name="path" type="s" direction="in"/>
			<arg name="isDerivated" type="b" direction="in"/>
			<arg name="prefix" type="s" direction="in"/>
			<arg name="filename" type="s" direction="in"/>
		</method>
		<method name="openProject">
			<arg name="filename" type="s" direction="in"/>
		</method>
		<method name="closeProjectNoSessionData"/>
		<method name="closeProjectWithSessionData"/>

		<method name="updateFromVersionManager">
			<arg name="list" type="as"/>
		</method>
		<method name="commitToVersionManager">
			<arg name="list" type="as"/>
		</method>
		<method name="addFilesToVersionManager">
			<arg name="list" type="as"/>
		</method>
		<method name="removeFilesFromVersionManager">
			<arg name="list" type="as"/>
		</method>
  </interface>
</node>

